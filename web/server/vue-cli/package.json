{
  "name": "codechecker",
  "version": "1.0.0",
  "description": "CodeChecker viewer.",
  "author": "Márton Csordás",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "server:codechecker": "CodeChecker server -w e2e/__codechecker/workspace",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --config config/webpack.dev.js",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server",
    "test:clean": "npm run test:e2e.clean",
    "test:e2e": "npm run test:e2e.firefox",
    "test:e2e.clean": "rimraf e2e/output e2e/screenshots e2e/__codechecker/workspace e2e/__codechecker/reports e2e/__codechecker/compilation_database",
    "test:e2e.chrome": "server-test test:e2e.server http-get://localhost:8001/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.chrome'",
    "test:e2e.firefox": "server-test test:e2e.server http-get://localhost:8001/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.firefox'",
    "test:e2e.init-db": "node e2e/init.db.js",
    "test:e2e.init-reports": "node e2e/init.reports.js",
    "test:e2e.init-workspace": "node e2e/init.workspace.js",
    "test:e2e.server": "npm run test:e2e.init-reports && rimraf e2e/__codechecker/workspace && npm run test:e2e.init-workspace && npm run server:codechecker",
    "test:lint": "eslint --ext .js,.vue src",
    "test:nightwatch": "nightwatch",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "codechecker-api": "6.26.0",
    "codemirror": "^5.52.0",
    "date-fns": "^2.10.0",
    "highlight.js": "^9.18.1",
    "jsplumb": "^2.12.9",
    "lodash": "^4.17.15",
    "marked": "^0.8.0",
    "splitpanes": "^2.2.1",
    "vue": "^2.6.11",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@mdi/font": "^4.9.95",
    "@types/jest": "^25.1.3",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "chromedriver": "^80.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "cross-env": "^6.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.2.1",
    "fibers": "^4.0.2",
    "file-loader": "^5.1.0",
    "geckodriver": "^1.19.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "nightwatch": "^1.3.4",
    "raw-loader": "^4.0.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "sass": "^1.26.0",
    "sass-loader": "^8.0.2",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.10.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.14",
    "vuetify-loader": "^1.4.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "@cc/report-server-types": "codechecker-api/lib/report_server_types.js"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
